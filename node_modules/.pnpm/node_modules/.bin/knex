#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/solal/Documents/GitHub/stream-chain/node_modules/.pnpm/knex@0.21.21_pg@8.12.0/node_modules/knex/bin/node_modules:/Users/solal/Documents/GitHub/stream-chain/node_modules/.pnpm/knex@0.21.21_pg@8.12.0/node_modules/knex/node_modules:/Users/solal/Documents/GitHub/stream-chain/node_modules/.pnpm/knex@0.21.21_pg@8.12.0/node_modules:/Users/solal/Documents/GitHub/stream-chain/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/solal/Documents/GitHub/stream-chain/node_modules/.pnpm/knex@0.21.21_pg@8.12.0/node_modules/knex/bin/node_modules:/Users/solal/Documents/GitHub/stream-chain/node_modules/.pnpm/knex@0.21.21_pg@8.12.0/node_modules/knex/node_modules:/Users/solal/Documents/GitHub/stream-chain/node_modules/.pnpm/knex@0.21.21_pg@8.12.0/node_modules:/Users/solal/Documents/GitHub/stream-chain/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../knex/bin/cli.js" "$@"
else
  exec node  "$basedir/../knex/bin/cli.js" "$@"
fi
