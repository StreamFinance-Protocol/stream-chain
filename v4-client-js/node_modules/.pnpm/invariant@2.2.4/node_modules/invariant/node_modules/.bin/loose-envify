#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/ec2-user/ethos-integration/stream-chain/e2e-testing/__tests__/helpers/v4-client-js/node_modules/.pnpm/loose-envify@1.4.0/node_modules/loose-envify/node_modules:/home/ec2-user/ethos-integration/stream-chain/e2e-testing/__tests__/helpers/v4-client-js/node_modules/.pnpm/loose-envify@1.4.0/node_modules:/home/ec2-user/ethos-integration/stream-chain/e2e-testing/__tests__/helpers/v4-client-js/node_modules/.pnpm/node_modules:/home/ec2-user/ethos-integration/stream-chain/e2e-testing/__tests__/helpers/v4-client-js/node_modules:/home/ec2-user/ethos-integration/stream-chain/e2e-testing/__tests__/helpers/node_modules:/home/ec2-user/ethos-integration/stream-chain/e2e-testing/__tests__/node_modules:/home/ec2-user/ethos-integration/stream-chain/e2e-testing/node_modules:/home/ec2-user/ethos-integration/stream-chain/node_modules:/home/ec2-user/ethos-integration/node_modules:/home/ec2-user/node_modules:/home/node_modules:/node_modules:/home/ec2-user/ethos-integration/stream-chain/e2e-testing/__tests__/helpers/v4-client-js/node_modules/.pnpm/invariant@2.2.4/node_modules/invariant/node_modules:/home/ec2-user/ethos-integration/stream-chain/e2e-testing/__tests__/helpers/v4-client-js/node_modules/.pnpm/invariant@2.2.4/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/ec2-user/ethos-integration/stream-chain/e2e-testing/__tests__/helpers/v4-client-js/node_modules/.pnpm/loose-envify@1.4.0/node_modules/loose-envify/node_modules:/home/ec2-user/ethos-integration/stream-chain/e2e-testing/__tests__/helpers/v4-client-js/node_modules/.pnpm/loose-envify@1.4.0/node_modules:/home/ec2-user/ethos-integration/stream-chain/e2e-testing/__tests__/helpers/v4-client-js/node_modules/.pnpm/node_modules:/home/ec2-user/ethos-integration/stream-chain/e2e-testing/__tests__/helpers/v4-client-js/node_modules:/home/ec2-user/ethos-integration/stream-chain/e2e-testing/__tests__/helpers/node_modules:/home/ec2-user/ethos-integration/stream-chain/e2e-testing/__tests__/node_modules:/home/ec2-user/ethos-integration/stream-chain/e2e-testing/node_modules:/home/ec2-user/ethos-integration/stream-chain/node_modules:/home/ec2-user/ethos-integration/node_modules:/home/ec2-user/node_modules:/home/node_modules:/node_modules:/home/ec2-user/ethos-integration/stream-chain/e2e-testing/__tests__/helpers/v4-client-js/node_modules/.pnpm/invariant@2.2.4/node_modules/invariant/node_modules:/home/ec2-user/ethos-integration/stream-chain/e2e-testing/__tests__/helpers/v4-client-js/node_modules/.pnpm/invariant@2.2.4/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../loose-envify@1.4.0/node_modules/loose-envify/cli.js" "$@"
else
  exec node  "$basedir/../../../../../loose-envify@1.4.0/node_modules/loose-envify/cli.js" "$@"
fi
