#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/ea/Desktop/Roxom/staking/stream-chain/v4-client-js/node_modules/.pnpm/shelljs@0.8.5/node_modules/shelljs/bin/node_modules:/Users/ea/Desktop/Roxom/staking/stream-chain/v4-client-js/node_modules/.pnpm/shelljs@0.8.5/node_modules/shelljs/node_modules:/Users/ea/Desktop/Roxom/staking/stream-chain/v4-client-js/node_modules/.pnpm/shelljs@0.8.5/node_modules:/Users/ea/Desktop/Roxom/staking/stream-chain/v4-client-js/node_modules/.pnpm/node_modules:/Users/ea/Desktop/Roxom/staking/stream-chain/v4-client-js/node_modules:/Users/ea/Desktop/Roxom/staking/stream-chain/node_modules:/Users/ea/Desktop/Roxom/staking/node_modules:/Users/ea/Desktop/Roxom/node_modules:/Users/ea/Desktop/node_modules:/Users/ea/node_modules:/Users/node_modules:/node_modules:/Users/ea/Desktop/Roxom/staking/stream-chain/v4-client-js/node_modules/.pnpm/node_modules/shelljs/bin/node_modules:/Users/ea/Desktop/Roxom/staking/stream-chain/v4-client-js/node_modules/.pnpm/node_modules/shelljs/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/ea/Desktop/Roxom/staking/stream-chain/v4-client-js/node_modules/.pnpm/shelljs@0.8.5/node_modules/shelljs/bin/node_modules:/Users/ea/Desktop/Roxom/staking/stream-chain/v4-client-js/node_modules/.pnpm/shelljs@0.8.5/node_modules/shelljs/node_modules:/Users/ea/Desktop/Roxom/staking/stream-chain/v4-client-js/node_modules/.pnpm/shelljs@0.8.5/node_modules:/Users/ea/Desktop/Roxom/staking/stream-chain/v4-client-js/node_modules/.pnpm/node_modules:/Users/ea/Desktop/Roxom/staking/stream-chain/v4-client-js/node_modules:/Users/ea/Desktop/Roxom/staking/stream-chain/node_modules:/Users/ea/Desktop/Roxom/staking/node_modules:/Users/ea/Desktop/Roxom/node_modules:/Users/ea/Desktop/node_modules:/Users/ea/node_modules:/Users/node_modules:/node_modules:/Users/ea/Desktop/Roxom/staking/stream-chain/v4-client-js/node_modules/.pnpm/node_modules/shelljs/bin/node_modules:/Users/ea/Desktop/Roxom/staking/stream-chain/v4-client-js/node_modules/.pnpm/node_modules/shelljs/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../shelljs/bin/shjs" "$@"
else
  exec node  "$basedir/../shelljs/bin/shjs" "$@"
fi
