#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/ec2-user/stream-chain/v4-client-js/node_modules/.pnpm/grpc-tools@1.12.4/node_modules/grpc-tools/bin/node_modules:/home/ec2-user/stream-chain/v4-client-js/node_modules/.pnpm/grpc-tools@1.12.4/node_modules/grpc-tools/node_modules:/home/ec2-user/stream-chain/v4-client-js/node_modules/.pnpm/grpc-tools@1.12.4/node_modules:/home/ec2-user/stream-chain/v4-client-js/node_modules/.pnpm/node_modules:/home/ec2-user/stream-chain/v4-client-js/node_modules:/home/ec2-user/stream-chain/node_modules:/home/ec2-user/node_modules:/home/node_modules:/node_modules:/home/ec2-user/stream-chain/v4-client-js/node_modules/grpc-tools/bin/node_modules:/home/ec2-user/stream-chain/v4-client-js/node_modules/grpc-tools/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/ec2-user/stream-chain/v4-client-js/node_modules/.pnpm/grpc-tools@1.12.4/node_modules/grpc-tools/bin/node_modules:/home/ec2-user/stream-chain/v4-client-js/node_modules/.pnpm/grpc-tools@1.12.4/node_modules/grpc-tools/node_modules:/home/ec2-user/stream-chain/v4-client-js/node_modules/.pnpm/grpc-tools@1.12.4/node_modules:/home/ec2-user/stream-chain/v4-client-js/node_modules/.pnpm/node_modules:/home/ec2-user/stream-chain/v4-client-js/node_modules:/home/ec2-user/stream-chain/node_modules:/home/ec2-user/node_modules:/home/node_modules:/node_modules:/home/ec2-user/stream-chain/v4-client-js/node_modules/grpc-tools/bin/node_modules:/home/ec2-user/stream-chain/v4-client-js/node_modules/grpc-tools/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../grpc-tools/bin/protoc_plugin.js" "$@"
else
  exec node  "$basedir/../grpc-tools/bin/protoc_plugin.js" "$@"
fi
