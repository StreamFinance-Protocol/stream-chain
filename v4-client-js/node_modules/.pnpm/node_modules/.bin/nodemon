#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/ec2-user/ethos-integration/stream-chain/v4-client-js/node_modules/.pnpm/nodemon@2.0.22/node_modules/nodemon/bin/node_modules:/home/ec2-user/ethos-integration/stream-chain/v4-client-js/node_modules/.pnpm/nodemon@2.0.22/node_modules/nodemon/node_modules:/home/ec2-user/ethos-integration/stream-chain/v4-client-js/node_modules/.pnpm/nodemon@2.0.22/node_modules:/home/ec2-user/ethos-integration/stream-chain/v4-client-js/node_modules/.pnpm/node_modules:/home/ec2-user/ethos-integration/stream-chain/v4-client-js/node_modules:/home/ec2-user/ethos-integration/stream-chain/node_modules:/home/ec2-user/ethos-integration/node_modules:/home/ec2-user/node_modules:/home/node_modules:/node_modules:/home/ec2-user/ethos-integration/stream-chain/v4-client-js/node_modules/.pnpm/node_modules/nodemon/bin/node_modules:/home/ec2-user/ethos-integration/stream-chain/v4-client-js/node_modules/.pnpm/node_modules/nodemon/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/ec2-user/ethos-integration/stream-chain/v4-client-js/node_modules/.pnpm/nodemon@2.0.22/node_modules/nodemon/bin/node_modules:/home/ec2-user/ethos-integration/stream-chain/v4-client-js/node_modules/.pnpm/nodemon@2.0.22/node_modules/nodemon/node_modules:/home/ec2-user/ethos-integration/stream-chain/v4-client-js/node_modules/.pnpm/nodemon@2.0.22/node_modules:/home/ec2-user/ethos-integration/stream-chain/v4-client-js/node_modules/.pnpm/node_modules:/home/ec2-user/ethos-integration/stream-chain/v4-client-js/node_modules:/home/ec2-user/ethos-integration/stream-chain/node_modules:/home/ec2-user/ethos-integration/node_modules:/home/ec2-user/node_modules:/home/node_modules:/node_modules:/home/ec2-user/ethos-integration/stream-chain/v4-client-js/node_modules/.pnpm/node_modules/nodemon/bin/node_modules:/home/ec2-user/ethos-integration/stream-chain/v4-client-js/node_modules/.pnpm/node_modules/nodemon/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../nodemon/bin/nodemon.js" "$@"
else
  exec node  "$basedir/../nodemon/bin/nodemon.js" "$@"
fi
