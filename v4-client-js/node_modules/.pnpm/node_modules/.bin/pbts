#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/ec2-user/ethos-integration/stream-chain/v4-client-js/node_modules/.pnpm/@pyramation+protobufjs@6.11.5/node_modules/@pyramation/protobufjs/bin/node_modules:/home/ec2-user/ethos-integration/stream-chain/v4-client-js/node_modules/.pnpm/@pyramation+protobufjs@6.11.5/node_modules/@pyramation/protobufjs/node_modules:/home/ec2-user/ethos-integration/stream-chain/v4-client-js/node_modules/.pnpm/@pyramation+protobufjs@6.11.5/node_modules/@pyramation/node_modules:/home/ec2-user/ethos-integration/stream-chain/v4-client-js/node_modules/.pnpm/@pyramation+protobufjs@6.11.5/node_modules:/home/ec2-user/ethos-integration/stream-chain/v4-client-js/node_modules/.pnpm/node_modules:/home/ec2-user/ethos-integration/stream-chain/v4-client-js/node_modules:/home/ec2-user/ethos-integration/stream-chain/node_modules:/home/ec2-user/ethos-integration/node_modules:/home/ec2-user/node_modules:/home/node_modules:/node_modules:/home/ec2-user/ethos-integration/stream-chain/v4-client-js/node_modules/.pnpm/node_modules/@pyramation/protobufjs/bin/node_modules:/home/ec2-user/ethos-integration/stream-chain/v4-client-js/node_modules/.pnpm/node_modules/@pyramation/protobufjs/node_modules:/home/ec2-user/ethos-integration/stream-chain/v4-client-js/node_modules/.pnpm/node_modules/@pyramation/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/ec2-user/ethos-integration/stream-chain/v4-client-js/node_modules/.pnpm/@pyramation+protobufjs@6.11.5/node_modules/@pyramation/protobufjs/bin/node_modules:/home/ec2-user/ethos-integration/stream-chain/v4-client-js/node_modules/.pnpm/@pyramation+protobufjs@6.11.5/node_modules/@pyramation/protobufjs/node_modules:/home/ec2-user/ethos-integration/stream-chain/v4-client-js/node_modules/.pnpm/@pyramation+protobufjs@6.11.5/node_modules/@pyramation/node_modules:/home/ec2-user/ethos-integration/stream-chain/v4-client-js/node_modules/.pnpm/@pyramation+protobufjs@6.11.5/node_modules:/home/ec2-user/ethos-integration/stream-chain/v4-client-js/node_modules/.pnpm/node_modules:/home/ec2-user/ethos-integration/stream-chain/v4-client-js/node_modules:/home/ec2-user/ethos-integration/stream-chain/node_modules:/home/ec2-user/ethos-integration/node_modules:/home/ec2-user/node_modules:/home/node_modules:/node_modules:/home/ec2-user/ethos-integration/stream-chain/v4-client-js/node_modules/.pnpm/node_modules/@pyramation/protobufjs/bin/node_modules:/home/ec2-user/ethos-integration/stream-chain/v4-client-js/node_modules/.pnpm/node_modules/@pyramation/protobufjs/node_modules:/home/ec2-user/ethos-integration/stream-chain/v4-client-js/node_modules/.pnpm/node_modules/@pyramation/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@pyramation/protobufjs/bin/pbts" "$@"
else
  exec node  "$basedir/../@pyramation/protobufjs/bin/pbts" "$@"
fi
