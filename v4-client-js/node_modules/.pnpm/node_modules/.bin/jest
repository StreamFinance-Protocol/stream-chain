#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/ec2-user/ethos-removal/stream-chain/v4-client-js/node_modules/.pnpm/jest@28.1.3_@types+node@14.18.63/node_modules/jest/bin/node_modules:/home/ec2-user/ethos-removal/stream-chain/v4-client-js/node_modules/.pnpm/jest@28.1.3_@types+node@14.18.63/node_modules/jest/node_modules:/home/ec2-user/ethos-removal/stream-chain/v4-client-js/node_modules/.pnpm/jest@28.1.3_@types+node@14.18.63/node_modules:/home/ec2-user/ethos-removal/stream-chain/v4-client-js/node_modules/.pnpm/node_modules:/home/ec2-user/ethos-removal/stream-chain/v4-client-js/node_modules:/home/ec2-user/ethos-removal/stream-chain/node_modules:/home/ec2-user/ethos-removal/node_modules:/home/ec2-user/node_modules:/home/node_modules:/node_modules:/home/ec2-user/ethos-removal/stream-chain/v4-client-js/node_modules/.pnpm/node_modules/jest/bin/node_modules:/home/ec2-user/ethos-removal/stream-chain/v4-client-js/node_modules/.pnpm/node_modules/jest/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/ec2-user/ethos-removal/stream-chain/v4-client-js/node_modules/.pnpm/jest@28.1.3_@types+node@14.18.63/node_modules/jest/bin/node_modules:/home/ec2-user/ethos-removal/stream-chain/v4-client-js/node_modules/.pnpm/jest@28.1.3_@types+node@14.18.63/node_modules/jest/node_modules:/home/ec2-user/ethos-removal/stream-chain/v4-client-js/node_modules/.pnpm/jest@28.1.3_@types+node@14.18.63/node_modules:/home/ec2-user/ethos-removal/stream-chain/v4-client-js/node_modules/.pnpm/node_modules:/home/ec2-user/ethos-removal/stream-chain/v4-client-js/node_modules:/home/ec2-user/ethos-removal/stream-chain/node_modules:/home/ec2-user/ethos-removal/node_modules:/home/ec2-user/node_modules:/home/node_modules:/node_modules:/home/ec2-user/ethos-removal/stream-chain/v4-client-js/node_modules/.pnpm/node_modules/jest/bin/node_modules:/home/ec2-user/ethos-removal/stream-chain/v4-client-js/node_modules/.pnpm/node_modules/jest/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../jest/bin/jest.js" "$@"
else
  exec node  "$basedir/../jest/bin/jest.js" "$@"
fi
