#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/solal/Documents/GitHub/stream-chain/node_modules/.pnpm/tsoa@5.1.1/node_modules/tsoa/dist/node_modules:/Users/solal/Documents/GitHub/stream-chain/node_modules/.pnpm/tsoa@5.1.1/node_modules/tsoa/node_modules:/Users/solal/Documents/GitHub/stream-chain/node_modules/.pnpm/tsoa@5.1.1/node_modules:/Users/solal/Documents/GitHub/stream-chain/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/solal/Documents/GitHub/stream-chain/node_modules/.pnpm/tsoa@5.1.1/node_modules/tsoa/dist/node_modules:/Users/solal/Documents/GitHub/stream-chain/node_modules/.pnpm/tsoa@5.1.1/node_modules/tsoa/node_modules:/Users/solal/Documents/GitHub/stream-chain/node_modules/.pnpm/tsoa@5.1.1/node_modules:/Users/solal/Documents/GitHub/stream-chain/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../dist/cli.js" "$@"
else
  exec node  "$basedir/../../dist/cli.js" "$@"
fi
