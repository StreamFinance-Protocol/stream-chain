#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/alejandrogala/UhtredsCave/stream/core/stream-chain/v4-client-js/node_modules/.pnpm/envinfo@7.14.0/node_modules/envinfo/dist/node_modules:/Users/alejandrogala/UhtredsCave/stream/core/stream-chain/v4-client-js/node_modules/.pnpm/envinfo@7.14.0/node_modules/envinfo/node_modules:/Users/alejandrogala/UhtredsCave/stream/core/stream-chain/v4-client-js/node_modules/.pnpm/envinfo@7.14.0/node_modules:/Users/alejandrogala/UhtredsCave/stream/core/stream-chain/v4-client-js/node_modules/.pnpm/node_modules:/Users/alejandrogala/UhtredsCave/stream/core/stream-chain/v4-client-js/node_modules:/Users/alejandrogala/UhtredsCave/stream/core/stream-chain/node_modules:/Users/alejandrogala/UhtredsCave/stream/core/node_modules:/Users/alejandrogala/UhtredsCave/stream/node_modules:/Users/alejandrogala/UhtredsCave/node_modules:/Users/alejandrogala/node_modules:/Users/node_modules:/node_modules:/Users/alejandrogala/UhtredsCave/stream/core/stream-chain/v4-client-js/node_modules/.pnpm/node_modules/envinfo/dist/node_modules:/Users/alejandrogala/UhtredsCave/stream/core/stream-chain/v4-client-js/node_modules/.pnpm/node_modules/envinfo/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/alejandrogala/UhtredsCave/stream/core/stream-chain/v4-client-js/node_modules/.pnpm/envinfo@7.14.0/node_modules/envinfo/dist/node_modules:/Users/alejandrogala/UhtredsCave/stream/core/stream-chain/v4-client-js/node_modules/.pnpm/envinfo@7.14.0/node_modules/envinfo/node_modules:/Users/alejandrogala/UhtredsCave/stream/core/stream-chain/v4-client-js/node_modules/.pnpm/envinfo@7.14.0/node_modules:/Users/alejandrogala/UhtredsCave/stream/core/stream-chain/v4-client-js/node_modules/.pnpm/node_modules:/Users/alejandrogala/UhtredsCave/stream/core/stream-chain/v4-client-js/node_modules:/Users/alejandrogala/UhtredsCave/stream/core/stream-chain/node_modules:/Users/alejandrogala/UhtredsCave/stream/core/node_modules:/Users/alejandrogala/UhtredsCave/stream/node_modules:/Users/alejandrogala/UhtredsCave/node_modules:/Users/alejandrogala/node_modules:/Users/node_modules:/node_modules:/Users/alejandrogala/UhtredsCave/stream/core/stream-chain/v4-client-js/node_modules/.pnpm/node_modules/envinfo/dist/node_modules:/Users/alejandrogala/UhtredsCave/stream/core/stream-chain/v4-client-js/node_modules/.pnpm/node_modules/envinfo/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../envinfo/dist/cli.js" "$@"
else
  exec node  "$basedir/../envinfo/dist/cli.js" "$@"
fi
