#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/solal/Documents/GitHub/stream-chain/node_modules/.pnpm/@cosmwasm+ts-codegen@0.21.1/node_modules/@cosmwasm/ts-codegen/main/node_modules:/Users/solal/Documents/GitHub/stream-chain/node_modules/.pnpm/@cosmwasm+ts-codegen@0.21.1/node_modules/@cosmwasm/ts-codegen/node_modules:/Users/solal/Documents/GitHub/stream-chain/node_modules/.pnpm/@cosmwasm+ts-codegen@0.21.1/node_modules/@cosmwasm/node_modules:/Users/solal/Documents/GitHub/stream-chain/node_modules/.pnpm/@cosmwasm+ts-codegen@0.21.1/node_modules:/Users/solal/Documents/GitHub/stream-chain/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/solal/Documents/GitHub/stream-chain/node_modules/.pnpm/@cosmwasm+ts-codegen@0.21.1/node_modules/@cosmwasm/ts-codegen/main/node_modules:/Users/solal/Documents/GitHub/stream-chain/node_modules/.pnpm/@cosmwasm+ts-codegen@0.21.1/node_modules/@cosmwasm/ts-codegen/node_modules:/Users/solal/Documents/GitHub/stream-chain/node_modules/.pnpm/@cosmwasm+ts-codegen@0.21.1/node_modules/@cosmwasm/node_modules:/Users/solal/Documents/GitHub/stream-chain/node_modules/.pnpm/@cosmwasm+ts-codegen@0.21.1/node_modules:/Users/solal/Documents/GitHub/stream-chain/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../@cosmwasm+ts-codegen@0.21.1/node_modules/@cosmwasm/ts-codegen/main/ts-codegen.js" "$@"
else
  exec node  "$basedir/../../../../../../@cosmwasm+ts-codegen@0.21.1/node_modules/@cosmwasm/ts-codegen/main/ts-codegen.js" "$@"
fi
