#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/ec2-user/ethos-integration/stream-chain/e2e-testing/__tests__/helpers/v4-client-js/node_modules/.pnpm/jsesc@0.5.0/node_modules/jsesc/bin/node_modules:/home/ec2-user/ethos-integration/stream-chain/e2e-testing/__tests__/helpers/v4-client-js/node_modules/.pnpm/jsesc@0.5.0/node_modules/jsesc/node_modules:/home/ec2-user/ethos-integration/stream-chain/e2e-testing/__tests__/helpers/v4-client-js/node_modules/.pnpm/jsesc@0.5.0/node_modules:/home/ec2-user/ethos-integration/stream-chain/e2e-testing/__tests__/helpers/v4-client-js/node_modules/.pnpm/node_modules:/home/ec2-user/ethos-integration/stream-chain/e2e-testing/__tests__/helpers/v4-client-js/node_modules:/home/ec2-user/ethos-integration/stream-chain/e2e-testing/__tests__/helpers/node_modules:/home/ec2-user/ethos-integration/stream-chain/e2e-testing/__tests__/node_modules:/home/ec2-user/ethos-integration/stream-chain/e2e-testing/node_modules:/home/ec2-user/ethos-integration/stream-chain/node_modules:/home/ec2-user/ethos-integration/node_modules:/home/ec2-user/node_modules:/home/node_modules:/node_modules:/home/ec2-user/ethos-integration/stream-chain/e2e-testing/__tests__/helpers/v4-client-js/node_modules/.pnpm/regjsparser@0.1.5/node_modules/regjsparser/node_modules:/home/ec2-user/ethos-integration/stream-chain/e2e-testing/__tests__/helpers/v4-client-js/node_modules/.pnpm/regjsparser@0.1.5/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/ec2-user/ethos-integration/stream-chain/e2e-testing/__tests__/helpers/v4-client-js/node_modules/.pnpm/jsesc@0.5.0/node_modules/jsesc/bin/node_modules:/home/ec2-user/ethos-integration/stream-chain/e2e-testing/__tests__/helpers/v4-client-js/node_modules/.pnpm/jsesc@0.5.0/node_modules/jsesc/node_modules:/home/ec2-user/ethos-integration/stream-chain/e2e-testing/__tests__/helpers/v4-client-js/node_modules/.pnpm/jsesc@0.5.0/node_modules:/home/ec2-user/ethos-integration/stream-chain/e2e-testing/__tests__/helpers/v4-client-js/node_modules/.pnpm/node_modules:/home/ec2-user/ethos-integration/stream-chain/e2e-testing/__tests__/helpers/v4-client-js/node_modules:/home/ec2-user/ethos-integration/stream-chain/e2e-testing/__tests__/helpers/node_modules:/home/ec2-user/ethos-integration/stream-chain/e2e-testing/__tests__/node_modules:/home/ec2-user/ethos-integration/stream-chain/e2e-testing/node_modules:/home/ec2-user/ethos-integration/stream-chain/node_modules:/home/ec2-user/ethos-integration/node_modules:/home/ec2-user/node_modules:/home/node_modules:/node_modules:/home/ec2-user/ethos-integration/stream-chain/e2e-testing/__tests__/helpers/v4-client-js/node_modules/.pnpm/regjsparser@0.1.5/node_modules/regjsparser/node_modules:/home/ec2-user/ethos-integration/stream-chain/e2e-testing/__tests__/helpers/v4-client-js/node_modules/.pnpm/regjsparser@0.1.5/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../jsesc@0.5.0/node_modules/jsesc/bin/jsesc" "$@"
else
  exec node  "$basedir/../../../../../jsesc@0.5.0/node_modules/jsesc/bin/jsesc" "$@"
fi
